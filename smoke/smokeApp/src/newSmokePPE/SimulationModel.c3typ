entity type SimulationModel mixes SimModelBase schema name "SIM_MDL" {
    //
    ensembles: [SimulationEnsemble](model,id)
    //
    GprTrainingTechniques: [GprPredictionModelParameters](model,id)
    /**
    */
    @py(env='gordon_1_0_0')
    createTrainingTechnique: inline member function(
        kernelName: !string, 
        serializedKernel: !string, 
        featureExcludeList: [string], 
        featureIncludeList: [string], 
        simulationExcludeList: [integer],
        simulationIncludeList: [integer],
        targetName: string = 'sumAll',
        randomSeed: integer = 42, 
        centerTarget: boolean = false, 
        validation: boolean = false, 
        splitFraction: float = 0.2
        ): GprPredictionModelParameters
    
}