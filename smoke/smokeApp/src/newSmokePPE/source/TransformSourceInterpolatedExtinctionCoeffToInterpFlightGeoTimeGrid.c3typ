type TransformSourceInterpolatedExtinctionCoeffToInterpFlightGeoTimeGrid mixes InterpFlightGeoTimeGrid transforms SourceInterpolatedExtinctionCoeff {
    id: ~ expression "concat(string(latitude),'_',string(longitude),'_',string(time))"
    dataset: ~ expression {id: "'smoke_ppe_tatz_flight'"}
    time: ~ expression "dateTime(time)"
    latitude: ~ expression "latitude"
    longitude: ~ expression "longitude"
}