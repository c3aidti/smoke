@db(index=['latitude,longitude', 'time','dataset'],compactType=true)
entity type InterpFlightGeoTimeGrid schema name 'INTRP_FLT_GTG' {
    //
    dataset: !InterpFlightSimulationDataset
    // the latitude
    latitude: !double
    // the longitude
    longitude: !double
    // the timestamp
    time: !datetime
    // The PostGIS enabled geo point (long,lat) of this output
    geo: !GeoPoint
}